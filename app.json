{
  "expo": {
    "name": "Shopit",
    "slug": "shopit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f0f0"

    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gustavosilva.shopapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Shop listing to keep track of what you need to buy."
  }
}
